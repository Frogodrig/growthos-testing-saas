{
  "name": "growthos-testing-saas",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "packages/*",
    "services/*",
    "apps/*"
  ],
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "db:generate": "prisma generate --schema=infrastructure/prisma/schema.prisma",
    "db:migrate": "prisma migrate dev --schema=infrastructure/prisma/schema.prisma",
    "db:push": "prisma db push --schema=infrastructure/prisma/schema.prisma",
    "docker:up": "docker compose -f infrastructure/docker-compose.yml up -d",
    "docker:down": "docker compose -f infrastructure/docker-compose.yml down"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "prisma": "^5.9.0",
    "turbo": "^2.0.0",
    "typescript": "^5.3.0"
  },
  "packageManager": "npm@11.6.2",
  "engines": {
    "node": ">=20.0.0"
  }
}
